{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/src/content/blog"
    }
  ],
  "frontMatter.content.publicFolder": {
    "path": "src/assets",
    "relative": true
  },
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "pubDate",
          "name": "pubDate",
          "type": "datetime"
        },
        {
          "title": "heroImage",
          "name": "heroImage",
          "type": "image"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "authors",
          "name": "authors",
          "type": "choice",
          "choices": [
            "TechnoHead"
          ]
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        }
      ]
    }
  ]
}